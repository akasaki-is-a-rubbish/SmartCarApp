{
  "name": "smartcar",
  "version": "0.2.5",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "postinstall": "patch-package",
    "lint": "eslint .",
    "np": "np --no-publish --target android",
    "postversion": "react-native-version",
    "build:tailwind": "tailwindcss --input input.css --output tailwind.css --no-autoprefixer && tailwind-rn",
    "dev:tailwind": "concurrently \"tailwindcss --input input.css --output tailwind.css --no-autoprefixer --watch\" \"tailwind-rn --watch\""
  },
  "dependencies": {
    "@georstat/react-native-image-cache": "^2.0.1",
    "@react-native-async-storage/async-storage": "^1.17.0",
    "@react-native-community/cli-platform-android": "^7.0.1",
    "@react-native-community/geolocation": "^2.0.2",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/native-stack": "^6.5.0",
    "axios": "^0.26.1",
    "buffer": "^6.0.3",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "react": "17.0.2",
    "react-content-loader": "^6.2.0",
    "react-native": "0.67.4",
    "react-native-basic-form": "^1.1.11",
    "react-native-camera": "^3.3.0",
    "react-native-echarts-pro": "1.7.4",
    "react-native-elements": "^3.4.2",
    "react-native-file-access": "^2.4.1",
    "react-native-gesture-handler": "^2.3.2",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-permissions": "^3.3.1",
    "react-native-progress": "^5.0.0",
    "react-native-qrcode-scanner-view": "^2.0.0",
    "react-native-reanimated": "^2.5.0",
    "react-native-safe-area-context": "^4.2.2",
    "react-native-screens": "^3.13.1",
    "react-native-sound": "^0.11.2",
    "react-native-svg": "^12.3.0",
    "react-native-vector-icons": "^9.1.0",
    "react-native-webview": "^11.23.1",
    "react_native_mqtt": "^1.3.1",
    "tailwind-rn": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "concurrently": "^7.0.0",
    "eslint": "7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "np": "^7.6.1",
    "postcss": "^8.4.12",
    "react-native-version": "^4.0.0",
    "react-test-renderer": "17.0.2",
    "tailwindcss": "^3.0.23"
  },
  "jest": {
    "preset": "react-native",
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [
      "node_modules/(?!@react-native|react-native|@react-navigation|tailwind-rn)"
    ],
    "setupFiles": [
      "./jest-setup.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akasaki-is-a-rubbish/SmartCar.git"
  }
}
